    "devDependencies": {
        "@types/cli-progress": "^3.7.0",
        "@types/mongoose": "^5.7.23",
        "@types/node": "^14.0.11",
        "@types/node-fetch": "^2.5.7",
        "@types/node-schedule": "^1.3.0",
        "concurrently": "^5.2.0",
        "nodemon": "^2.0.4",
        "typescript": "^3.9.5"
